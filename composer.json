{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "doctrine/dbal": "^2.10",
        "fadugyamfi/laravel-api-base": "dev-master",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "maatwebsite/excel": "4.0.x-dev",
        "nunomazer/laravel-samehouse": "dev-master",
        "orangehill/iseed": "^2.6",
        "propaganistas/laravel-phone": "dev-master",
        "spatie/laravel-permission": "^3.6",
        "staudenmeir/laravel-adjacency-list": "dev-master",
        "tymon/jwt-auth": "^1.0.0"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "knuckleswtf/scribe": "^3.0",
        "laracademy/generators": "^2.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./packages/*"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": false,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
