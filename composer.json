{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "doctrine/dbal": "^2.10",
        "fadugyamfi/laravel-api-base": "dev-master",
        "fideloper/proxy": "^4.0",
        "flexflux/laravel-elastic-email": "dev-master",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.6",
        "kkomelin/laravel-translatable-string-exporter": "dev-master",
        "laravel/framework": "^8.80",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "4.0.x-dev",
        "nunomazer/laravel-samehouse": "dev-master",
        "php-open-source-saver/jwt-auth": "dev-main",
        "propaganistas/laravel-phone": "dev-master",
        "spatie/laravel-activitylog": "4.1.1",
        "spatie/laravel-permission": "^3.6",
        "staudenmeir/laravel-adjacency-list": "^1.9.5"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.17",
        "knuckleswtf/scribe": "3.23",
        "laracademy/generators": "^2.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "orangehill/iseed": "dev-master",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
